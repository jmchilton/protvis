<tal:if condition="python: pep_datafile != None and pep_query_offset != None and pep_hit_offset != None">
	<iframe src="/lorikeet?file=${file}&n=${datafile}&type=${type}&spectrum=${spectrum}&off=${offset}&pn=${pep_datafile}&pqoff=${pep_query_offset}&phoff=${pep_hit_offset}" style="width:100%; height:613px; border:0px;"></iframe>
</tal:if>
<tal:else condition="python: pep_datafile == None or pep_query_offset == None or pep_hit_offset == None">
	<iframe src="/lorikeet?file=${file}&n=${datafile}&type=${type}&spectrum=${spectrum}&off=${offset}" style="width:100%; height:578px; border:0px;"></iframe>
</tal:else>
<script>SwitchDataset(${datafile}, "data_spectrum");</script>
